{
    "name": "eslint-plugin-no-restricted-syntax",
    "version": "1.0.0",
    "description": "An eslint plugin that adds two alternate rule names for the no-restricted-syntax rule, to allow some syntax to throw errors while others throw warnings.",
    "homepage": "https://github.com/8Crafter-Studios/eslint-plugin-no-restricted-syntax#readme",
    "bugs": {
        "email": "8crafteryt@gmail.com",
        "url": "https://github.com/8Crafter-Studios/eslint-plugin-no-restricted-syntax/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/8Crafter-Studios/eslint-plugin-no-restricted-syntax.git"
    },
    "license": "MIT",
    "author": {
        "email": "8crafteryt@gmail.com",
        "name": "8Crafter",
        "url": "https://www.8crafter.com"
    },
    "main": "./index.cjs",
    "types": "./index.d.ts",
    "module": "./index.mjs",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "module-sync": "./index.mjs",
            "require": "./index.cjs",
            "default": "./index.mjs"
        },
        "./packge.json": "./package.json",
        "./index": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "module-sync": "./index.mjs",
            "require": "./index.cjs",
            "default": "./index.mjs"
        },
        "./index.js": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "module-sync": "./index.mjs",
            "require": "./index.cjs",
            "default": "./index.mjs"
        },
        "./index.d.ts": "./index.d.ts",
        "./index.cjs": "./index.cjs",
        "./index.mjs": "./index.mjs"
    },
    "peerDependencies": {
        "@eslint/core": "*"
    },
    "devDependencies": {
        "eslint-plugin-eslint-plugin": "^7.3.0"
    }
}
